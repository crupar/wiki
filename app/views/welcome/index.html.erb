<div class="jumbotron">

	<h1>Carolyn's Wiki Iteration</h1>

	<% if current_user %>
		<h2> <%= link_to "Wiki Pages", wikipages_path(@wikipage) %> </h2>
		<%= link_to "Sign Out", destroy_user_session_path, method: :delete %>
	<% else %>
		<h2>Sign Up For An Account Today</h2>
		<%= link_to "Sign Up", new_user_registration_path, :class => "btn", :method => :get %>
	  <%= link_to "Log In", new_user_session_path, :class => "btn", :method => :get %>
	<% end %>

</div>
